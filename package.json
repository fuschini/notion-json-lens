{
  "name": "notion-json-converter",
  "module": "index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "version": "1.0.2",
  "description": "Bi-directional converter between Notion database properties format and a plain JSON object",
  "git repository": "https://github.com/fuschini/notion-json-lens",
  "keywords": [
    "notion",
    "object",
    "json",
    "database",
    "properties",
    "bi-directional",
    "converter"
  ],
  "test script": "bun test",
  "author": {
    "name": "Fuschini"
  },
  "license": "MIT",
  "devDependencies": {
    "bun-types": "latest",
    "bun-plugin-dts": "^0.2.1",
    "@types/bun": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "prepublishOnly": "bun run build",
    "build": "bun run cleanup && bun build --target=node ./src/index.ts --outfile=dist/index.js && bun run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json",
    "test": "bun test",
    "test-coverage": "bun test --coverage",
    "cleanup": "rm -f dist/* && rm -f tsconfig.types.tsbuildinfo"
  },
  "files": [
    "dist"
  ]
}